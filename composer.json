{
    "name": "devfusion/maker-bundle",
    "type": "symfony-bundle",
    "description": "This bundle is a code generation tool. It draws heavily on the symfony/maker-bundle by adding several features and more flexibility.",
    "homepage": "https://github.com/official-dev-fusion/MakerBundle",
    "license": "MIT",
    "keywords": ["generator", "code generator", "maker", "crud", "scrud", "skeleton", "symfony", "template", "override", "search", "create", "read", "update",    "delete", "filter", "pagination", "multi-select"],
    "authors": [
        {
            "name": "Martin GILBERT",
            "e-mail": "martin.gilbert@dev-fusion.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/maker-bundle": "^1.0",
        "symfony/twig-bundle": "^4.4"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "DF\\MakerBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
